{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "semi": "off",
    "prefer-const": "error",
    "no-trailing-spaces": "warn",
    "quotes": [
      "error",
      "double",
      {
        "avoidEscape": true
      }
    ],
    "no-console": [
      "error",
      {
        "allow": [
          "error"
        ]
      }
    ]
  }
}